	<script>
		$(function() {
			// 处理模态窗口缓存数据的问题
			$('body').on('hidden.bs.modal', '.modal', function() {
				$(this).removeData('bs.modal');
			});
			
			/*移除组内成员*/
			$(".fa.fa-pencil-square-o.apply").click(function(){
				if(confirm('是否要移除该组员')){
					var applyId=$(this).attr("id");
					var url="/controller/TaskService/verifyApply";
					$.post(url,{"verifyState":-2,"applyId":applyId,'isWinter':$("#isWinter").val()},function(result){
						/* if(result.returnCode==-3000){
							alert('非法操作');
						}else if(result.returnCode==-3009){
							alert('申请不存在');
						}else if(result.returnCode==-3004){
							alert('数据库错误');
						}else if(result.returnCode==0){
							alert('移除成功');
							var taskUuid=$("#taskUuid").val();
							location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/groupUserTask.html";
						} */
						if(result.returnCode==0){
							alert('移除成功');
							var taskUuid=$("#taskUuid").val();
							//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
							location.reload();
						}else{
							alert(result.returnMsg);
						}
					},'json');
				}
			});
			/*同意或者拒绝组内成员*/
			$(".fa.fa-pencil-square-o.able").click(function(){
				var applyId=$(this).attr("id");
				var url="/controller/TaskService/verifyApply";
				var verifyState=-1;
				if($(this).html()=="批准"){
					verifyState=1;
				}
				$.post(url,{"verifyState":verifyState,"applyId":applyId,"isWinter":$("#isWinter").val()},function(result){
					/* console.log(result.returnCode);
					if(result.returnCode==-3000){
						alert('非法操作');
					}else if(result.returnCode==-3009){
						alert('申请不存在');
					}else if(result.returnCode==-3004){
						alert('数据库错误');
					}else if(result.returnCode==0){
						alert('审批成功');
						var taskUuid=$("#taskUuid").val();
						location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/groupUserTask.html";
					} */
					if(result.returnCode==0){
						alert('审批成功');
						var taskUuid=$("#taskUuid").val();
						//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
						location.reload();
					}else{
						alert(result.returnMsg);
					}
				},'json').error(function(result){
					alert('对方手机号可能有误！发送短信失败');
					var taskUuid=$("#taskUuid").val();
					//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
					location.reload();
				});
			});
			/*移除临时成员*/
			$(".fa.fa-pencil-square-o.temp").click(function(){
				if(confirm('是否要移除该组员')){
					var tempId=$(this).attr("id");
					var url="/controller/TaskService/verifyTempMember";
					$.post(url,{"verifyStatus":-2,"tempId":tempId,"isWinter":$("#isWinter").val()},function(result){
						/* console.log(result.returnCode);
						if(result.returnCode==-3000){
							alert('非法操作');
						}else if(result.returnCode==-3009){
							alert('申请不存在');
						}else if(result.returnCode==-3004){
							alert('数据库错误');
						}else if(result.returnCode==0){
							alert('移除成功');
							var taskUuid=$("#taskUuid").val();
							location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/groupUserTask.html";
						} */
						if(result.returnCode==0){
							alert('移除成功');
							var taskUuid=$("#taskUuid").val();
							location.reload();
							//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
						}else{
							alert(result.returnMsg);
						}
					},'json');
				}
			});
			/*批准与拒绝临时组员*/
			$(".fa.fa-pencil-square-o.tempable").click(function(){
					var tempId=$(this).attr("id");
					var url="/controller/TaskService/verifyTempMember";
					var verifyStatus=-1;
					if($(this).html()=="批准"){
						verifyStatus=1;
					}
					$.post(url,{"verifyStatus":verifyStatus,"tempId":tempId,"isWinter":$("#isWinter").val()},function(result){
						/* console.log(result.returnCode);
						if(result.returnCode==-3000){
							alert('非法操作');
						}else if(result.returnCode==-3009){
							alert('申请不存在');
						}else if(result.returnCode==-3004){
							alert('数据库错误');
						}else if(result.returnCode==0){
							alert('审批成功');
							var taskUuid=$("#taskUuid").val();
							location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/groupUserTask.html";
						} */
						if(result.returnCode==0){
							alert('审批成功');
							var taskUuid=$("#taskUuid").val();
							//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
							location.reload();
						}else{
							alert(result.returnMsg);
						}
					},'json');
			});
			/*搜索任务成人员*/
			$("#searchBy").on('submit',function(e){
				e.preventDefault();
				var url = $('#searchBy').prop("action");
				var Ident=$('#Ident').val();
				var taskid=$("#taskid").val();
				var groupId=$("#groupId").val();
				$.post(url,{'userRealName':Ident,'groupId':groupId,'taskid':taskid,'isWinter':$("#isWinter").val()},function(userlist, status){
					//console.log(userlist);
					if(userlist==""){
						alert("找不到符合条件的用户");
						return false;
					}
					$('#userLists').children().remove();
					$('#userLists').append(userlist);
				});
			});
			/*添加任务成员*/
			$("#addCaption").click(function(){
				var userUuid=$("#infoEdit input[name=userUuid]:checked").val();
				var taskid=$("#taskid").val();
				var url="/controller/TaskService/addTaskMemberByAdmin";
				$.post(url,{'userUuid':userUuid,'taskid':taskid,'isWinter':$("#isWinter").val()},function(result){
					var code=result.returnCode;
					console.log(code);
					/* if(code==-3000){
						alert('非法操作');
					}else if(code==-3006){
						alert('用户不存在');
					}else if(code==-3005){
						alert('任务不存在');
					}else if(code==-3004){
						alert('数据库错误');
					}else if(code==0){
						alert('添加任务人员成功');
						var taskUuid=$("#taskUuid").val();
						location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/groupUserTask.html";
					} */
					if(code==0){
						alert('添加任务人员成功');
						var taskUuid=$("#taskUuid").val();
						//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
						location.reload();
					}else{
						alert(result.returnMsg);
					}
				},'json');
				$('#captionModal').modal('hide');
			});
			/*提升任务负责人*/
			$(".upManager").click(function(e){
				if(confirm('是否将其提升为队长?')){
					var managerUuid=$(this).attr('id');
					var taskUuid=$("#taskUuid").val();
					var url="/controller/TaskService/modifyTaskManger";
					$.post(url,{"mangerUuid":managerUuid,"taskUuid":taskUuid,'isWinter':$("#isWinter").val()},function(result){
						var code=result.returnCode;
						if(code==0){
							alert('操作成功');
							//location.href="/controller/TaskService/getUsersByTask?tname=task/ownerUserTask.html&taskUuid="+taskUuid;
							location.reload();
						}else{
							alert(result.returnMsg);
						}
					},'json').error(function(result){
						alert('对方手机号可能有误！发送短信失败');
						var taskUuid=$("#taskUuid").val();
						//location.href="/controller/TaskService/getUsersByTask"+"?taskUuid="+taskUuid+"&tname=task/ownerUserTask.html";
						location.reload();
					});;
				}
				
			});
		});
	</script>
	
