	<script>
		$(function(){
			$(".fa.fa-chevron-right").click(function(){
				var lang=$(this).attr("lang");
				if(lang=="gonggao"){
					location.href="/page/news/noticeList?action=c2VydmljZU5hbWU9V2ViTmV3c1NlcnZpY2Umb3BlcmF0aW9uPWdldEdvbmdnYW9MaXN0&pageNo=1";
				}else if(lang=="xiazai"){
					location.href="/page/news/downloadList?action=c2VydmljZU5hbWU9V2ViTmV3c1NlcnZpY2Umb3BlcmF0aW9uPWdldFhpYXphaUxpc3Q=&pageNo=1";
				}else if(lang=="wenan"){
					location.href="/page/news/activityList?action=c2VydmljZU5hbWU9V2ViTmV3c1NlcnZpY2Umb3BlcmF0aW9uPWdldEh1b2RvbmdMaXN0&pageNo=1";
				}else if(lang=="contact"){
					location.href="/page/news/contactUs?action=c2VydmljZU5hbWU9V2ViTmV3c1NlcnZpY2Umb3BlcmF0aW9uPWdldExpYW54aUxpc3Q=&pageNo=1";
				}else{
