	<script>
		function showDetail(e) {
			//console.debug("点击了弹出按钮");
			var id = $(this).attr("id");
			id += "modal";
			console.debug("id: " + id);
			$('#' + id).modal();
			//$('#myModal').modal();
		}
		$(function() {
			$(".detail").bind("click",showDetail);
			
			// 处理模态窗口缓存数据的问题
			$('body').on('hidden.bs.modal', '.modal', function() {
				$(this).removeData('bs.modal');
			});

			

		});
	</script>
	
