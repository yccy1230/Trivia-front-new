var ad_title_list = '自制驴打滚,如何学剪纸,金鱼吃什么,叠纸大全,王菲,水培仙人球';
var ad_url_list = 'http://inte.sogou.com/skwd?query=%D7%D4%D6%C6%C2%BF%B4%F2%B9%F6&&pid=sogou-wsse-63154d5661f774fb&pos=1&exd=0;0;11;-1;2;0;0,http://inte.sogou.com/skwd?query=%C8%E7%BA%CE%D1%A7%BC%F4%D6%BD&&pid=sogou-wsse-63154d5661f774fb&pos=2&exd=0;0;11;-1;2;0;0,http://inte.sogou.com/skwd?query=%BD%F0%D3%E3%B3%D4%CA%B2%C3%B4&&pid=sogou-wsse-63154d5661f774fb&pos=3&exd=0;0;11;-1;2;0;0,http://inte.sogou.com/skwd?query=%B5%FE%D6%BD%B4%F3%C8%AB&&pid=sogou-wsse-63154d5661f774fb&pos=4&exd=0;0;11;-1;2;0;0,http://inte.sogou.com/skwd?query=%CD%F5%B7%C6&&pid=sogou-wsse-63154d5661f774fb&pos=5&exd=0;0;11;-1;2;0;0,http://inte.sogou.com/skwd?query=%CB%AE%C5%E0%CF%C9%C8%CB%C7%F2&&pid=sogou-wsse-63154d5661f774fb&pos=6&exd=0;0;11;-1;2;0;0';
var ad_img_list = 'http://img01.sogoucdn.com/app/a/200630/4c03e7cf4e61f9a7f176c52bf2cf9be1,http://img04.sogoucdn.com/app/a/200630/5492d6310783e7dc625ea793f40fe138,http://img01.sogoucdn.com/app/a/200630/5b6cd2212ad8927b97d061388111e7d3,http://img04.sogoucdn.com/app/a/200630/79f08a84571416ed89ec9fa390e61d0b,http://img03.sogoucdn.com/app/a/200630/015ce211391b6396993e0f261931dd1e,http://img02.sogoucdn.com/app/a/200630/a9cfbc9c8762566a41a543e6b2d2d2d9';
var financial_tip ="";

var ad_title = ad_title_list.split(',');
var ad_url = ad_url_list.split(',');
var ad_img = ad_img_list.split(',');
/*
 *  需要留下的接口：
 *  图片区：尺寸大小，文字位置，文字透明度，背景颜色，边框颜色
 *  内容区：字体大小，字体样式，字体颜色，有无下划线
