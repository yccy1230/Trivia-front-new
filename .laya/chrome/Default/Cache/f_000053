var iframe_height = 250;
var iframe_width = 300;
var border = 1;
var picHover="pic-hover-"+1;


var pic_org_height = 248;
var text_org_height = 0;
var logo_flag = "6";
/*新增logo逻辑*/
var logo_class = "logo_hide";
logo_flag = Number(logo_flag);
switch(logo_flag){
   case 0:
     logo_class = "logo_hide";
     break;
   case 1:
     logo_class = "logo";
     break;
   case 2:
     logo_class = "logo_text1";
     break;
   case 3:
     logo_class = "logo_text2";
     break;
   case 4:
     logo_class = "logo_text4";
     break;
   case 5:
     logo_class = "logo_text5";
     break;
   case 6:
     logo_class = "logo_text6";
     break;
   default:
     logo_class = "logo_hide";
}
/*end 新增logo逻辑*/

var ad_itor = 0;

var addHandler = function(element, type, listener) {
    type = type.replace(/^on/i, '').toLowerCase();

    // 事件监听器挂载
    if (element.addEventListener) {
        element.addEventListener(type, listener, false);
    } else if (element.attachEvent) {
        element.attachEvent('on' + type, listener);
    }
    return element;
};
var slice_title = [];
(function(){
   //if  base_width less 88 ,max title size is 6
   var cur_base_width = 97;
   if(cur_base_width > 88){
     slice_title = ad_title;
     return;
   }
   for(var i = 0; i < ad_title.length;i++){
       slice_title[i] = ad_title[i].slice(0,6);
   }
})()
var render_pic_org = function(){
    if(!pic_org_height){
        return;
    }
    var pic_border = 1;

    var pic_height = pic_org_height;
    var pic_width = iframe_width - 2*border;

    var pic_base_h = 102 - 2*pic_border;
    var pic_base_w = 97 - 2*pic_border;

    var is_outside = 1;

    var pic_des_h = 20;
    if(is_outside){
        var base_h = pic_base_h + pic_des_h;
        var base_w = pic_base_w;
    }else{
        var base_h = pic_base_h;
        var base_w = pic_base_w;
    }

    var pic_row_num = 2;
    var pic_col_num = 3;

    var pic_row_h = Math.floor(pic_height/pic_row_num);
    var pic_row_w = pic_width;
    var pic_item_w = Math.floor(pic_width/pic_col_num);
    var pic_org_content = document.createElement('div');
    pic_org_content.className = 'pic-org-content';
    for(var i = 0; i < pic_row_num; i ++){
        var pic_row = document.createElement('div');
        pic_row.className = 'pic-row';
        pic_row.style.height = pic_row_h + 'px';
        pic_row.style.width = pic_row_w + 'px';
        for(var j = 0; j < pic_col_num; j++){
            var pic_item = document.createElement('div');
            var pic = document.createElement('div');
            pic.className = 'pic';
                var pic_item_margin_x = Math.floor((pic_width - pic_col_num*(base_w + pic_border*2))/pic_col_num/2);
                var pic_item_margin_y = Math.floor((pic_height - pic_row_num*(base_h + pic_border*2))/pic_row_num/2);

                pic_item.className = 'pic-item';
                pic_item.style.margin = pic_item_margin_y + 'px ' + pic_item_margin_x + 'px ' + pic_item_margin_y + 'px ' + pic_item_margin_x + 'px';

                pic_item.style.cursor = 'pointer';
                /* add hover */
                addHandler(pic_item,"mouseenter",function(pic_item, className) {
                            return function(eve) {
                                pic_item.className = 'pic-item '+className;
                            };
                        } (pic_item, picHover)
                )
                addHandler(pic_item,"mouseleave",function(pic_item, className) {
                            return function(eve) {
                                pic_item.className = 'pic-item';
                            };
                        } (pic_item, picHover)
                )
                var pic_a = document.createElement('a');
                pic_a.setAttribute('target','_blank');
                pic_a.setAttribute('href',ad_url[ad_itor]);
                var img = document.createElement('img');
                img.setAttribute('src',ad_img[ad_itor]);
                img.style.height = pic_base_h + 'px';
                img.style.width = pic_base_w + 'px';
                pic_a.setAttribute('title',ad_title[ad_itor]);
                pic_a.appendChild(img);



                // 金融beginner
                var tip_parent = document.createElement('div');
                tip_parent.className = 'tip_parent';
                // 将img跟div设置为同级目录，其父元素为a标签,为了正常显示tip，将a标签的属性全部去掉
                tip_parent.innerHTML = '<div class="tip"><img class="tip_icon_1" src="http://p.inte.sogou.com/testgpimg/pc_tip.png" /><img class="tip_icon_2" src="http://p.inte.sogou.com/testgpimg/pc_tip_on.png" /></div>';
                pic_a.appendChild(tip_parent);
                //  金融end



            if(is_outside){

                pic.appendChild(pic_a);
                pic_item.appendChild(pic);

                var pic_des = document.createElement('div');
                pic_des.className = 'pic-des-outside';
                //pic_des.innerHTML = ad_title[ad_itor];
                pic_des.style.lineHeight = pic_des_h + 'px';
                pic_des.style.height = pic_des_h + 'px';
                pic_des.style.width = pic_base_w + 'px';
                var des_a = document.createElement('a');
                des_a.innerHTML = slice_title[ad_itor]; //change to use slice_title
                //des_a.style.color = '#000000';    //去掉颜色 通过变量统一定义
                des_a.setAttribute('target','_blank');
                des_a.setAttribute('href',ad_url[ad_itor]);
                des_a.setAttribute('title',ad_title[ad_itor]);
                pic_des.appendChild(des_a);

                pic_item.appendChild(pic_des);
                pic_row.appendChild(pic_item);
//                addHandler(pic_item,'mouseenter',function(pic,des){return function(eve){
//                    pic.className = 'pic pic-hover';
//                    des.style.color = '#FF0000';
//                };}(pic,pic_des));
//                addHandler(pic_item,'mouseleave',function(pic,des){return function(eve){
//                    pic.className = 'pic';
//                    des.style.color = '#000000';
//                };}(pic,pic_des));
            }
            else{
                var pic_des = document.createElement('div');
                pic_des.className = 'pic-des-inside';
                pic_des.innerHTML = ad_title[ad_itor];
                pic_des.style.lineHeight = pic_des_h + 'px';
                pic_des.style.height = pic_des_h + 'px';
                pic_des.style.width = pic_base_w + 'px';
                pic_a.appendChild(pic_des);

                pic.appendChild(pic_a);
                pic_item.appendChild(pic);
                pic_row.appendChild(pic_item);
//                addHandler(pic,'mouseenter',function(eve){
//                    var ele = event.target || event.srcElement;
//                    ele.className = 'pic pic-hover';
//                });
//                addHandler(pic,'mouseleave',function(eve){
//                    var ele = event.target || event.srcElement;
//                    ele.className = 'pic';
//                });
            }

            ad_itor++;

        }
        pic_org_content.appendChild(pic_row);
    }
    return pic_org_content;
};

var render_text_org = function(){
    if(!text_org_height){
        return;
    }
    var text_margin = 15;
    var text_height = text_org_height;
    var text_width = iframe_width - 2*border;
    //写死
    var text_base_h = 30;
    //写死，最大七个字
    var text_base_w = 120;

    var text_li_num = Math.floor(text_height/text_base_h);
    var text_row_num = Math.floor(text_width/text_base_w);
    if(text_row_num%2 == 0){
        var text_ul_num = 2;
    }else{
        var text_ul_num = 1;
    }

    var text_li_a_num = text_row_num / text_ul_num;
    var bold_arr = [1,0,1];
    var b_itor = 0;
    var text_org_content = document.createElement('div');
    text_org_content.className = 'text-org-content';
    for(var i = 0; i < text_ul_num; i ++){
        var text_ul = document.createElement('ul');
        text_ul.style.width = text_width/text_ul_num + 'px';
        for(var j = 0; j < text_li_num; j++){
            var text_li = document.createElement('li');
            text_li.style.width = text_width/text_ul_num + 'px';
            text_li.style.height = text_base_h + 'px';
            text_li.style.padding = '0 0 0 '+text_margin+'px';
            for(var k = 0 ; k < text_li_a_num; k++){
                var text_li_a = document.createElement('a');
                text_li_a.innerHTML = ad_title[ad_itor];
                text_li_a.setAttribute('href',ad_url[ad_itor]);
                text_li_a.setAttribute('target','_blank');
                text_li_a.style.height = text_base_h + 'px';
                text_li_a.style.lineHeight = text_base_h + 'px';
                if(bold_arr[b_itor]){
                    text_li_a.style.fontWeight = 600;
                }
                if( k > 0 ){
                    text_li_a.style.margin = '0 0 0 '+text_margin+'px';
                }
                addHandler(text_li_a,'mouseenter',function(eve){
                    var ele = event.target || event.srcElement;
                    ele.className = 'text-hover';
                });
                addHandler(text_li_a,'mouseleave',function(eve){
                    var ele = event.target || event.srcElement;
                    ele.className = '';
                });
                ad_itor++;
                b_itor++;
                text_li.appendChild(text_li_a);
            }
            text_ul.appendChild(text_li);
        }
        text_org_content.appendChild(text_ul);
    }
    return text_org_content;
};

var render_page = function(){
    var ad_container = document.createElement('div');
    ad_container.id = "div_ad";
    ad_container.style.height = iframe_height - 2*border + 'px';
    ad_container.style.width = iframe_width - 2*border + 'px';
    ad_container.className = 'ad-container';
    ad_container.appendChild(render_pic_org());
    //ad_container.appendChild(render_text_org());

    //add logo
     var logo_dom = document.createElement('div');
     logo_dom.innerHTML = '<a id="logo" class="logo '+logo_class+'" href="http://union.sogou.com/?" style="z-index:2" target="_blank"></a>';
     ad_container.appendChild(logo_dom);//采用append,使用innerhtml会导致事件失效

     document.body.appendChild(ad_container);
};

render_page();

// 金融提示begin
function bind(elem,type,handler)
 {
      if(document.addEventListener)
      {
       elem.addEventListener(type,handler,false);
      }else if(elem.attachEvent){
       elem.attachEvent('on'+type,handler);
      }else{
       elem['on'+type] = handler;
      }
 }
function getElements(context,className){
    if(document.getElementsByClassName){
        return context.getElementsByClassName(className);
    }else{
        var arr = [];
        var allNodes = context.getElementsByTagName('*');
        for(var i=0;i<allNodes.length;i++){
            if(allNodes[i].className == className){
                arr.push(allNodes[i]);
            }
        }
        return arr;
    }
}

function start_tip(){
    //开启金融提示

    // 获得要处理的图片数组
    var tip_list = (financial_tip+ '').split(',');
    if(tip_list[0]== ''){
      return;
    }

    var all_tip_parent = getElements(document,"tip_parent");
    var all_tip = getElements(document,"tip");
    var all_tip_icon_1 = getElements(document,"tip_icon_1");
    var all_tip_icon_2 = getElements(document,"tip_icon_2");
    if(all_tip.length <=0){
        return;
    }
    for(var i =0; i<all_tip.length;i++){
        if(typeof tip_list[i] !="undefined" && tip_list[i] == 1){
            // 是否命中展示金融图片
            (function(){
                var tip_parent = all_tip_parent[i];
                var tip =all_tip[i];
                var tip_icon_1 = all_tip_icon_1[i];
                var tip_icon_2 = all_tip_icon_2[i];


                bind(tip_icon_1,"mouseover",function(e){
                   tip_icon_1.style.display = "none";
                   tip_icon_2.style.display = "block";

                });
                bind(tip_icon_2,"mouseout",function(e){
                   tip_icon_1.style.display = "block";
                   tip_icon_2.style.display = "none";

                });
                tip_parent.className += " tip_parent_show";
            })(i)
        }

    }

};
setTimeout(function(){
    start_tip();
},500);
