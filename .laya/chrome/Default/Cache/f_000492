	<script>
		function apply(e){
			var uuid=$(this).attr("id");
			//console.debug("获取到id: " + uuid);
			var flag=$(this).html();
			var isWinter=$("#isWinter").val();
			console.log(isWinter);
			
			var url= "/controller/TaskService";
			if(isWinter==1){
				url= "/controller/WinterTaskService";
			}
			if(flag == "申请"){
				var accept = confirm("是否申请该任务？");
				if (!accept) {
					return;
				}
				$(this).html("撤销");
				url += "/addApply?taskUuid=" + uuid;
				//url += "/deleteApply?taskUuid=" + uuid;
				$.post(url,function(data, status) {
					var error = "操作失败";
					var jsonObj = JSON.parse(data);
					//console.log(jsonObj);
					if (jsonObj.returnCode < 0) {
						$('#' + uuid).html(flag);
						if (jsonObj.returnCode == -3008) {
							$('#' + uuid).html("撤销");
							error += ",您已申请过该任务";
						} else if (jsonObj.returnCode == -3010) {
							error += "，非待审状态，无法撤销";
						}else if(jsonObj.returnCode == -3012){
							alert(jsonObj.returnMsg);
						}
						alert(error);
					}else{
						alert('申请成功');
					}
					location.href="/page/task/groupNewMission?action=c2VydmljZU5hbWU9VGFza1NlcnZpY2Umb3BlcmF0aW9uPWdldE15R3JvdXBPdGhlclRhc2tz";
				});
			} else if (flag == "撤销") {
				var accept = confirm("是否撤销对该任务的申请？");
				if (!accept) {
					return;
				}
				$(this).html("申请");
				url += "/deleteApply?taskUuid=" + uuid;
				$.post(url,function(data, status) {
					var error = "操作失败";
					var jsonObj = JSON.parse(data);
					//console.log(jsonObj);
					if (jsonObj.returnCode < 0) {
						$('#' + uuid).html(flag);
						if (jsonObj.returnCode == -3008) {
							$('#' + uuid).html("撤销");
							error += ",您已申请过该任务";
						} else if (jsonObj.returnCode == -3010) {
							error += "，非待审状态，无法撤销";
						}
						alert(error);
					}else{
						alert('撤销成功');
						location.href="/page/task/groupNewMission?action=c2VydmljZU5hbWU9VGFza1NlcnZpY2Umb3BlcmF0aW9uPWdldE15R3JvdXBPdGhlclRhc2tz";
					}
				});
			}
			
		}
		$(function() {
			pageList();
			$(document).on('click','.apply',apply);
			// 处理模态窗口缓存数据的问题
			$('body').on('hidden.bs.modal', '.modal', function() {
				$(this).removeData('bs.modal');
			});
			$("#searchTask").submit(function(e){
				e.preventDefault();
				var province=$("#province").val();
				var city=$("#city").val();
				var data=$("#searchTask").serialize();
				$.post($("#searchTask").attr("action"),data,function(resultList){
					try{
		    			if(resultList==""||resultList==undefined){
		    				alert("未检索到相关任务,请重新选择");
		    				return false;
		    			}
						$("#province2").val(province);
		    			$("#city2").val(city);
		    			$("#navCreate").attr("data-flag",1);
		    			$('#resultList').replaceWith(resultList);
		    			pageList();
		    		}catch(e){
		    			console.log(e);
		    		}
				});
			});
			$("#navCreate").click(function(e){
				var flag=$("#navCreate").attr("data-flag");
				if(flag==0){
					if(confirm("警告：建议您先检索查看已有的任务，以免重复创建任务!\n确定继续创建吗?")){
						location.href="/page/task/ownerTaskList?action=c2VydmljZU5hbWU9VGFza1NlcnZpY2Umb3BlcmF0aW9uPWdldE15QXBwbHlUYXNr";
					}else{
						return false;
					}
				}else{
					location.href="/page/task/ownerTaskList?action=c2VydmljZU5hbWU9VGFza1NlcnZpY2Umb3BlcmF0aW9uPWdldE15QXBwbHlUYXNr";
				}
			});
			//省市级联
			$("#province").change(function(e){
				var p=$(this).val();
				if(p==""){
					return false;
				}
				$.post("/controller/ProvinceService/getCitys",{provinceId:p},function(data){
					console.log(data);
					if(data.returnCode==0){
						var c=$("#city");
						var cityList=data.citys;
						c.empty().append('<option value="-1">请选择城市</option>');
						for(var i in cityList){
							c.append("<option value='"+cityList[i].areaId+"'>"+cityList[i].areaName+"</option>");
						}
					}else{
						alert(data.returnMsg);
					}
				},'json');
			});
		});
		function pageList(){
			/*分页*/
			var tp=$("#totalPage").val();
			var options = {
					alignment:"center",//居中显示
				    bootstrapMajorVersion : 3, // bootstrap.css文件版本
				    currentPage : $("#p").val(), // 当前页数
				    numberOfPages: tp<5?tp:tp>10?10:tp,//一页显示几个按钮（在ul里面生成5个li）
				    totalPages : tp, // 总页数
				    itemTexts : function (type, page, current) {
				        switch (type) {
				            case "first":
				                return "首页";
				            case "prev":
				                return "上一页";
				            case "next":
				                return "下一页";
				            case "last":
				                return "末页";
				            case "page":
				                return page;
				        }
				    },
				    onPageClicked : function (event, originalEvent, type, page) {
				    	$('#p').val(page);
				    	$.post($('#pageForm').prop("action"),$('#pageForm').serialize(), function(questions, status) {
				    		try{
				    			$('#resultList').replaceWith(questions);
				    		}catch(e){
				    			console.log(e);
				    		}
							$(window).scrollTop(0);
				    	});
				    }
				};
			$('#page').bootstrapPaginator(options);
		}
	</script>
	
